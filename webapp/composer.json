{
    "name": "jp3cki/fetusjp",
    "description": "fetus.jp",
    "homepage": "https://fetus.jp/",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.0",
        "jp3cki/uuid": "^2.0.0",
        "jp3cki/yii2-ubuntu-font": "^1.0.0",
        "rmrevin/yii2-fontawesome": "^3.6.0",
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-bootstrap4": "*"
    },
    "require-dev": {
        "ext-posix": "*",

        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-yii2": "^1.1",
        "codeception/specify": "^1.4",
        "codeception/verify": "^2.1",
        "dominikb/composer-license-checker": "^2.1",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpstan/phpstan": "^0.12.88",
        "proget-hq/phpstan-yii2": "^0.7.4",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "^5.3",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.3"
    },
    "replace": {
        "bower-asset/bootstrap": "*",
        "bower-asset/inputmask": "*",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*",
        "npm-asset/bootstrap": "*"
    },
    "config": {
        "process-timeout": 1800,
        "platform": {
            "php": "8.0.6"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "test -e ./vendor/bin/codecept && ./vendor/bin/codecept build",
            "./yii license/clean-extracted",
            "./yii license/extract"
        ],
        "post-update-cmd": [
            "test -e ./vendor/bin/codecept && ./vendor/bin/codecept build",
            "./yii license/clean-extracted",
            "./yii license/extract"
        ]
    }
}
